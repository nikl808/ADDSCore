<Application x:Class="ADDSCore.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ADDSCore"
             xmlns:ACSEdit ="clr-namespace:ADDSCore.Dialog.ACSEditDialog"
             xmlns:ACSEditHw ="clr-namespace:ADDSCore.Dialog.ACSEditHwDialog"
             xmlns:ACSEditParam ="clr-namespace:ADDSCore.Dialog.ACSEditParamDialog"
             xmlns:Email ="clr-namespace:ADDSCore.Dialog.EmailDialog"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type ACSEdit:ACSQuestEditMainViewModel}">
            <ACSEdit:ACSQuestEditMainDialog/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type ACSEditHw:ACSEditHwViewModel}">
            <ACSEditHw:ACSEditHwDialog/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type ACSEditParam:ACSEditParamViewModel}">
            <ACSEditParam:ACSEditParamDialog/>
        </DataTemplate>
         <DataTemplate DataType="{x:Type Email:SendEmailViewModel}">
             <Email:SendEmailDialog/>
         </DataTemplate>
    </Application.Resources>
</Application>
